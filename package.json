    "fuse.js": "3.4.4",
    "html-webpack-plugin": "^5.5.3",
    "js-cookie": "2.2.0",
    "js-md5": "0.7.3",
    "jsonlint": "1.6.3",
    "jszip": "^3.10.1",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
